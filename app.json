{
  "expo": {
    "name": "SalesLoop",
    "slug": "sales-loop",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SalesLoop to access your camera"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "associatedDomains": ["webcredentials:sales-loop.jackcrane.rocks"],
      "buildNumber": "1",
      "bundleIdentifier": "com.jackcrane.SalesLoop"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "997b62da-125e-4ae8-bc0d-dfd207b57174"
      }
    }
  }
}
